{
  "name": "@mlxr/typescript-sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for MLXR - High-performance LLM inference on Apple Silicon",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mlx",
    "llm",
    "inference",
    "openai",
    "ollama",
    "apple-silicon",
    "m1",
    "m2",
    "m3",
    "m4",
    "macos",
    "typescript",
    "ai",
    "ml"
  ],
  "author": "MLXR Development",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LayerDynamics/MLXR.git",
    "directory": "sdks/typescript"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
